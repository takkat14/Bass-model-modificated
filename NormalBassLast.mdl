{UTF-8}
"CL-Comp"=
	tr1 * WoM Adoption * Competitors * p21 * Clients * (1 - p11 - k1 * p13)
	~	
	~		|

"CL-PC"=
	Clients * p13 * k1
	~	
	~		|

Clients= INTEG (
	"Comp-CL"+"PC-CL"-"CL-Comp"-"CL-PC",
		0)
	~	
	~		|

"Comp-CL"=
	tr2 * WoM Adoption * Competitors * p11 * Clients * (1 - p21 - k2 * p23)
	~	
	~		|

"Comp-PC"=
	Competitors * p23 * k2
	~	
	~		|

Competitors= INTEG (
	"CL-Comp"+"PC-Comp"-"Comp-CL"-"Comp-PC",
		0)
	~	
	~		|

effMarket1=
	0.011
	~	
	~		|

effMarket2=
	effMarket1
	~	
	~		|

effWoM=
	0.015
	~	
	~		|

k1=
	(effMarket1 + effMarket2) / (2 * effWoM + effMarket1 + effMarket2)
	~	
	~		|

k2=
	(effMarket1 + effMarket2) / (2 * effWoM + effMarket1 + effMarket2)
	~	
	~		|

p11=
	0.15
	~	 [0,1]
	~		|

p13=
	0.15
	~	 [0,1]
	~		|

p21=
	0.15
	~	 [0,1]
	~		|

p23=
	0.15
	~	 [0,1]
	~		|

"PC-CL"=
	effMarket1 * Potential Clients + p11 * WoM Adoption * Potential Clients * Clients
	~	
	~		|

"PC-Comp"=
	effMarket2 * Potential Clients + p21 * WoM Adoption * Potential Clients * Competitors
	~	
	~		|

Potential Clients= INTEG (
	"Comp-PC"-"PC-Comp" + "CL-PC" - "PC-CL",
		Total Population)
	~	
	~		|

Social Rate=
	100
	~	person/Month
	~		|

Total Population=
	10^5
	~	
	~		|

tr1=
	(2 * effWoM) / (2 * effWoM + effMarket1 + effMarket2)
	~	
	~		|

tr2=
	tr1
	~	
	~		|

WoM Adoption=
	effWoM * Social Rate / Total Population
	~	
	~		|

